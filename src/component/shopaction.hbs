<div class="row">
    {{#with service}}
    <div class="card my-4 bg-success text-light text-center">
        <div class="card-body">
            <h2>{{title}}</h2>
            <p>{{description}}</p>
            {{#each buttons}}
            <a href="{{link}}" class="btn btn-primary">{{text}}</a>
            {{/each}}
        </div>
    </div>
    {{/with}}
</div>