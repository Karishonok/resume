<div class="col-12">
    <div class="my-4">
        <ol class="breadcrumb">
            {{#each breadcrumb}}
            <li class="breadcrumb-item">
                {{#if url}}<a href="{{url}}">{{name}}</a>
                {{else}}<span>{{name}}</span>
                {{/if}}
            </li>
            {{/each}}
        </ol>
    </div>
</div>