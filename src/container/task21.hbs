<main>
    <div class="card">
        <div class="card-body">
            {{#with heading}}
            {{#with main}}
            <h1>{{title}}</h1>
            <p>{{text}}</p>
            {{#with button_text}}
            <button class="btn btn-primary">{{solid}}</button>
            <br>
            <br>
            <button class="btn btn-outline-primary">{{outline}}</button>
            {{/with}}
            {{/with}}
        </div>
    </div>
    <br>
    <br>
    <br>
    <div class="card">
        <div class="card-body">
            {{#with second}}
            <h2>{{title}}</h2>
            <p>{{text}}</p>
            {{#with button_text}}
            <button class="btn btn-secondary">{{solid}}</button>
            <br>
            <br>
            <button class="btn btn-outline-secondary">{{outline}}</button>
            {{/with}}
            {{/with}}
        </div>
    </div>
    <br>
    <br>
    <br>
    <div class="card">
        <div class="card-body">
            {{#with subblock}}
            <h3>{{title}}</h3>
            <p>{{text}}</p>
            {{#with button_text}}
            <button class="btn btn-dark">{{solid}}</button>
            <br>
            <br>
            <button class="btn btn-outline-dark">{{outline}}</button>
            {{/with}}
            {{/with}}
        </div>
        {{/with}}
    </div>
    <br>
    <br>
    <br>
    {{#with nested_display}}
    <div class="display-1">{{title}}</div>
    <div class="card">
        <div class="card-body">
            {{#with children_display}}
            <div class="display-4">{{title}}</div>
            <p>{{text}}</p>
            {{#with button}}
            <button class="btn btn-outline-warning">{{text}}</button>
            {{/with}}
            {{/with}}
            <br>
            <br>
            <h6>{{header_text}}</h6>
            {{#with button}}
            <br>
            <button class="btn btn-info">{{text}}</button>
            {{/with}}
        </div>
    </div>
    {{/with}}
    <br>
    <br>
    <br>
    <div>
        {{#with paragraph}}
        <div class="text-uppercase">{{title}}</div>
        <div class="text-lowercase">{{text}}</div>
        {{#with button}}
        <br>
        <a href="https://google.com" class="btn btn-link">{{text}}</a>
        {{/with}}
        <br>
        <br>
        <div class="card">
            <div class="card-body">
                <ul>
                    {{#each list_name}}
                    <li>
                        <a href="https://github.com/" class="btn btn-link">{{text}}</a>
                    </li>
                    <br>
                    {{/each}}
                </ul>
            </div>
        </div>
        {{/with}}
    </div>
</main>