<div class="row p-3">
    <section class="my-5 text-bg-light">
        {{> title data= name big=true}}
        <def>{{description}}</def>
    </section>
    <section class="mb-3">
        {{> title data= "History"}}
        {{#with history}}
        <div class="my-2">Year: {{year}}</div>
        <div>Founder: {{founder}}</div>
        {{/with}}
    </section>
    <section>
        <div class="col-6 my-3">
            {{> title data="Types"}}
            {{> js parameter=types}}
        </div>
        <div class="col-6 my-3">
            {{> title data= "Syntax"}}
            {{#each syntax}}
            <b>{{@key}}</b>
            {{> js parameter=this}}
            {{/each}}
        </div>
        <div class="row mt-5">
            <div class="col-4">
                {{> title data="Features"}}
                {{> js parameter=features}}
            </div>
            <div class="col-4">
                {{> title data="Libraries"}}
                {{>js parameter=libraries}}
            </div>
            <div class="col-4">
                {{> title data="Tools"}}
                {{>js parameter=tools}}
            </div>
        </div>
    </section>
    <section class="my-4 text-bg-light">
        {{> title data= "Community"}}
        <ul>
            {{#each community}}
            <li>
                <b>{{name}}</b> <em>({{type}})</em>
            </li>
            {{/each}}
        </ul>
    </section>
</div>